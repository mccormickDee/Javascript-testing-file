dog.name = "Clifford";
dog.breed = "Labrador Retriever";
dog.color = "Red";
dog.show = "Clifford the Big Red Dog";
dog.sound = "I am intimidating because of my size";

window.alert("My dog is " + dog.name);
document.getElementById("demo").innerHTML = "My name is " + dog.name + " from the show called " + dog.show + ". " +  dog.sound + ", I also have bright " + dog.color + " fur!!"  ;

