myDog.color = "Red";
myDog.show = "Clifford the Big Red Dog";
myDog.sound = "I am intimidating because of my size";


document.getElementById("demo").innerHTML = "My name is " + myDog.name + " from the show called " + myDog.show + ". " +  myDog.sound + ", I also have bright " + myDog.color + " fur!!"  ;

